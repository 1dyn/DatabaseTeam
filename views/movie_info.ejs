	<script>
	var bDisplay = true;
        function doDisplay(movie){
            var con = document.getElementById(movie);
            con.style.display = "block";//해당 id를 가진 div 보이기
            var movies = ["all", "inter-conf", "inter-jour", "dom-conf", "dom-jour"];
            for (let i = 0; i < movies.length; i++) {//나머지 모든 div의 내용 숨기기
                if (movies[i]===movie) {
                    continue;
                }
                else{
                    con = document.getElementById(movies[i]);
                    con.style.display = "none";
                }
            }
        }
		</script>

